<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backg">

    <ImageButton
        android:id="@+id/messages_Main_id"
        android:layout_width="94dp"
        android:layout_height="76dp"
        android:background="@drawable/mensages"
        android:contentDescription="@string/Btn_test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/plus_Main_id"
        app:layout_constraintHorizontal_bias="0.576"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981"
        tools:ignore="ImageContrastCheck,RedundantDescriptionCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/profile_picture_Main_id"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="136dp"
        android:layout_height="104dp"
        android:adjustViewBounds="false"
        android:background="@drawable/profile"
        android:contentDescription="@string/profile_Picture"
        android:cropToPadding="true"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/plus_Main_id"
        android:layout_width="60dp"
        android:layout_height="65dp"
        android:background="@drawable/plus"
        android:contentDescription="@string/_plus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/settings_Main_Id"
        android:layout_width="102dp"
        android:layout_height="80dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/settings"
        android:contentDescription="@string/Adress"
        app:layout_constraintBottom_toBottomOf="parent"


        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.821"
        app:layout_constraintStart_toEndOf="@+id/plus_Main_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/qr_main_id"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="115dp"
        android:layout_height="110dp"
        android:background="@drawable/qr"
        android:backgroundTint="@color/white"
        android:clickable="true"
        android:contentDescription="@string/Btn_test"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095"
        app:tint="@color/teal_700"
        tools:ignore="RedundantDescriptionCheck,ImageContrastCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>